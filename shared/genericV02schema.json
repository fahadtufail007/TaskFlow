{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Task",
  "definitions": {
    "Privacy": {
      "type": "object",
      "additionalProperties": true,
      "title": "Privacy"
    },
    "Input": {
      "type": "object",
      "additionalProperties": true,
      "title": "Input"
    },
    "Meta": {
      "type": "object",
      "additionalProperties": true,
      "title": "Meta",
      "completedAt": {
        "type": "integer",
        "minimum": 0
      },
      "createdAt": {
        "type": "integer",
        "minimum": 0
      },
      "parentId": {
        "type": "string"
      },
      "parentInstanceId": {
        "type": "string"
      },
      "updateCount": {
        "type": "integer"
      },
      "updateRate": {
        "type": "integer"
      },
      "updatedAt": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "timezone": {
            "type": "string"
          }
        },
        "required": ["date", "timezone"]
      }
    },
    "Output": {
      "type": "object",
      "additionalProperties": true,
      "title": "Output"
    },
    "Config": {
      "type": "object",
      "additionalProperties": true,
      "title": "Configuration"
    },
    "Processor": {
      "type": "object",
      "additionalProperties": true,
      "title": "Processor"
    },    
    "Hub": {
      "type": "object",
      "additionalProperties": true,
      "title": "Hub"
    },    
    "Request": {
      "type": "object",
      "additionalProperties": true,
      "title": "Request"
    },
    "Response": {
      "type": "object",
      "additionalProperties": true,
      "title": "Response"
    },
    "State": {
      "type": "object",
      "additionalProperties": {},
      "properties": {
        "current": {
          "type": "string"
        },
        "last": {
          "type": "string"
        },
        "done": {
          "type": "boolean"
        },
        "id": {
          "type": "string"
        },
        "nextState": {
          "type": "string"
        }
      },
      "required": [],
      "title": "State"
    }
  }
}
